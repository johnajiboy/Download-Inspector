{
  "manifest_version": 3,
  "name": "Download Inspector",
  "version": "1.0",
  "permissions": ["activeTab"],
  "host_permissions": ["https://localhost:5000/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
